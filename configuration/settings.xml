<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          http://maven.apache.org/xsd/settings-1.0.0.xsd">
	  <profiles>
		<profile>
			<id>build.repos</id>
				<repositories>
				<repository>
					<id>cluster.local.ibm</id>
					<name>OpenShift Local Nexus</name>
					<url>http://nexus-internal.fuse-dev.svc.cluster.local:8081/repository/maven-all-public/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</repository>
				<repository>
					<id>cluster.local</id>
					<name>OpenShift Local Nexus</name>
					<url>http://nexus-internal.shared-services.svc.cluster.local:8081/repository/maven-all-public/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</repository>
				<repository>
					<id>redhat.ga</id>
					<name>Red Hat General Availability Repository</name>
					<url>https://maven.repository.redhat.com/ga</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</repository>
				<repository>
					<id>redhat.ea</id>
					<name>Red Hat Early Access Repository</name>
					<url>https://maven.repository.redhat.com/earlyaccess/all/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</repository>
				<repository>
					<id>central</id>
					<name>Maven Central</name>
					<url>http://uk.maven.org/maven2</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</repository>
			</repositories>
			<pluginRepositories>
			    <pluginRepository>
			     <id>cluster.local.ibm</id>
					<name>OpenShift Local Nexus</name>
					<url>http://nexus-internal.fuse-dev.svc.cluster.local:8081/repository/maven-all-public/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
			    </pluginRepository>
			    <pluginRepository>
			     <id>cluster.local</id>
					<name>OpenShift Local Nexus</name>
					<url>http://nexus-internal.shared-services.svc.cluster.local:8081/repository/maven-all-public/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
			    </pluginRepository>
				<pluginRepository>
					<id>redhat.ga</id>
					<name>Red Hat General Availability Repository</name>
					<url>https://maven.repository.redhat.com/ga</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</pluginRepository>
				<pluginRepository>
					<id>redhat.ea</id>
					<name>Red Hat Early Access Repository</name>
					<url>https://maven.repository.redhat.com/earlyaccess/all/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</pluginRepository>
				<pluginRepository>
					<id>central</id>
					<name>Maven Central</name>
					<url>http://uk.maven.org/maven2</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>build.repos</activeProfile>
	</activeProfiles>
</settings>



